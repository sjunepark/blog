{
  "name": "blog",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev-db": "turso dev --db-file db/local/local.db",
    "start": "astro dev",
    "build": "astro build",
    "preview": "wrangler pages dev dist",
    "astro": "astro",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "check": "astro check",
    "mg-up": "tsx db/migrate/up.ts",
    "mg-down": "tsx db/migrate/down.ts",
    "mg": "tsx db/migrate/migrate.ts",
    "mg-reset": "tsx db/migrate/reset.ts",
    "introspect": "kysely-codegen --url=\"env(DB_FULL_URL)\" --dialect=libsql",
    "introspect-local": "kysely-codegen --url=\"env(DEV_DB_URL)\" --dialect=libsql"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/cloudflare": "^10.0.2",
    "@astrojs/mdx": "^2.2.3",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.1.2",
    "@astrojs/svelte": "^5.3.0",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/ts-plugin": "^1.6.1",
    "@fontsource-variable/inter": "^5.0.17",
    "@fontsource-variable/lora": "^5.0.17",
    "@fontsource-variable/noto-sans-kr": "^5.0.5",
    "@formkit/tempo": "^0.0.19",
    "@libsql/kysely-libsql": "^0.3.0",
    "astro": "^4.5.14",
    "bits-ui": "^0.21.2",
    "clsx": "^2.1.0",
    "kysely": "^0.27.3",
    "mode-watcher": "^0.3.0",
    "svelte-radix": "^1.1.0",
    "tailwind-merge": "^2.2.2",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@shikijs/transformers": "^1.2.4",
    "@tailwindcss/typography": "^0.5.12",
    "@types/eslint": "^8.56.7",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.33.1",
    "eslint-plugin-svelte": "^2.35.1",
    "kysely-codegen": "^0.14.2",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-svelte": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "svelte": "^4.2.12",
    "tsx": "^4.7.1",
    "wrangler": "^3.44.0"
  }
}
